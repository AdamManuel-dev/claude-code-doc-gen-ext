        if (t.isIdentifier(parent.key)) {
          name = parent.key.name;
        } else if (t.isStringLiteral(parent.key)) {
          name = parent.key.value;
        }
      } else if (t.isAssignmentExpression(parent) && t.isIdentifier(parent.left)) {
        name = parent.left.name;
      }
