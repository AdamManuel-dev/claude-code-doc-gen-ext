    let name = '<anonymous>';
    let type = currentClass ? 'method' : 'function';
    
    // Get function name
    if (node.id) {
      name = node.id.name;
    } else {
      // Check parent for variable declarator
      const parent = path.parent;
      if (t.isVariableDeclarator(parent) && t.isIdentifier(parent.id)) {
        name = parent.id.name;
