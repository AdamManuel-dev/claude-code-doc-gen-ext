  FAIL: '\x1b[91m',
  ENDC: '\x1b[0m',
  BOLD: '\x1b[1m',
  UNDERLINE: '\x1b[4m'
};

// Supported file extensions
const SUPPORTED_EXTENSIONS = {
  '.js': 'jsdoc',
  '.jsx': 'jsdoc',
  '.ts': 'jsdoc',
  '.tsx': 'jsdoc',
  '.mjs': 'jsdoc',
  '.cjs': 'jsdoc'
};

